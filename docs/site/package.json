{
  "name": "material-ui-docs",
  "private": true,
  "version": "0.16.0",
  "description": "Documentation site for material-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/callemall/material-ui.git"
  },
  "scripts": {
    "start": "npm run clean && npm run start:dll && node webpack.dev.server.js",
    "start:dll": "webpack --config webpack.dll.config.js",
    "build": "npm run clean && npm run build:dll && cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "build:dll": "cross-env NODE_ENV=production webpack --config webpack.dll.prod.config.js",
    "clean": "rimraf build"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "img-loader": "^1.3.1",
    "object-assign": "^4.1.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "3.0.0-beta.6",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "marked": "^0.3.6",
    "prismjs": "^1.6.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-scroll": "^0.4.1",
    "react-swipeable-views": "^0.10.7",
    "redux": "^3.5.2"
  }
}
