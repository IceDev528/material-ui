{
  "extends": "../../tsconfig.json",
  "include": ["../types/react-docgen.d.ts", "buildApi.ts", "i18n.js"],
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": ["node"]
  },
  "exclude": ["node_modules"]
}
