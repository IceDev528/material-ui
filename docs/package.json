{
  "name": "material-ui-docs",
  "private": true,
  "version": "1.0.0",
  "description": "Documentation site for Material-UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/callemall/material-ui.git"
  },
  "scripts": {
    "start": "npm run clean && npm run start:dll && node webpack.dev.server.js",
    "start:dll": "webpack --config webpack.dll.config.js",
    "build": "npm run clean && npm run build:dll && cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "build:dll": "cross-env NODE_ENV=production webpack --config webpack.dll.prod.config.js",
    "clean": "rimraf build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "img-loader": "^2.0.0",
    "object-assign": "^4.1.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "3.0.0-beta.6",
    "redbox-react": "^1.3.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "marked": "^0.3.6",
    "prismjs": "^1.6.0",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "react-router-scroll": "^0.4.2",
    "react-swipeable-views": "^0.12.1",
    "recompose": "^0.23.1",
    "redux": "^3.5.2"
  }
}
