{
  "compilerOptions": {
    "outDir": "build/tsc",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": false,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "include": ["src/DatePicker/**/*", "__tests__/**/*.tsx"]
}
