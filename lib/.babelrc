{
  "presets": [
    ["@babel/env", { "modules": false, "loose": true }],
    "@babel/react"
  ],
  "plugins": [
    ["@babel/proposal-class-properties", { "loose": true }]
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/transform-runtime",
        "@babel/transform-modules-commonjs"
      ]
    },
    "cjs": {
      "plugins": [
        "@babel/transform-runtime",
        "@babel/transform-modules-commonjs"
      ]
    }
  }
}
