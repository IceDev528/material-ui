{
  "presets": [
    ["@babel/env", { "modules": false }],
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "@babel/proposal-class-properties"
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/transform-runtime",
        "@babel/transform-modules-commonjs"
      ]
    },
    "cjs": {
      "plugins": [
        "@babel/transform-runtime",
        "@babel/transform-modules-commonjs"
      ]
    },
    "esm": {
      "plugins": [
        "@babel/transform-runtime"
      ]
    }
  } 
}
