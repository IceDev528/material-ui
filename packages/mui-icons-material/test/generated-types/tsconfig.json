{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "baseUrl": "../../../",
    "noErrorTruncation": true,
    "paths": {
      "@mui/material": ["./mui-material/src"],
      "@mui/material/*": ["./mui-material/src/*"],
      "@mui/icons-material": ["./mui-icons-material/build"],
      "@mui/icons-material/*": ["./mui-icons-material/build/*"]
    }
  },
  "exclude": ["**/node_modules/"]
}
