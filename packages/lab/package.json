{
  "name": "material-ui-build-lab",
  "private": true,
  "author": "Material-UI Team",
  "version": "1.0.0-alpha.1",
  "description": "Material-UI Lab - Incubator for Material-UI React components.",
  "main": "./src/index.js",
  "keywords": [
    "Material",
    "Design",
    "Material-UI",
    "Lab",
    "React",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mui-org/material-ui.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mui-org/material-ui/issues"
  },
  "homepage": "https://github.com/mui-org/material-ui/tree/v1-beta/packages/lab",
  "scripts": {
    "start": "cd ../.. && yarn start",
    "docs:api": "cd ../.. && rimraf ./pages/lab/api/* && cross-env BABEL_ENV=docs-development babel-node ./docs/scripts/buildApi.js ./packages/lab/src ./pages/lab/api",
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --out-dir ./build",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./build/index.es.js",
    "build:copy-files": "babel-node ./copy-files.js",
    "build": "yarn build:es2015 && npm run build:es2015modules && npm run build:copy-files",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "lint:fix": "eslint . --cache --fix && echo \"eslint: no lint errors\"",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "cd ../.. && cross-env NODE_ENV=test mocha packages/lab/src/{,**/}*.spec.js",
    "test:watch": "yarn test:unit -w",
    "version": "yarn build",
    "release": "np --no-publish --any-branch",
    "postrelease": "npm publish build"
  },
  "peerDependencies": {
    "material-ui": "^1.0.0-beta.33",
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  },
  "dependencies": {
    "material-ui-icons": "^1.0.0-beta.17",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "app-module-path": "^2.0.0",
    "babel-cli": "^6.0.0",
    "chai": "^4.0.0",
    "cross-env": "^5.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-material-ui": "file:../eslint-plugin-material-ui",
    "eslint-plugin-mocha": "^4.0.0",
    "eslint-plugin-prettier": "^2.0.0",
    "eslint-plugin-react": "^7.0.0",
    "fs-extra": "^5.0.0",
    "mocha": "^5.0.0",
    "rimraf": "^2.0.0"
  }
}
