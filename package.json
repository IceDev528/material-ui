    "**/@babel/runtime": "^7.17.2",
    "**/@types/react": "^17.0.45",
    "**/@types/react-dom": "^17.0.17",
    "**/cross-fetch": "^3.1.5",
    "**/react-is": "^17.0.2"
  },
  "nyc": {
    "include": [
      "packages/mui*/src/**/*.{js,ts,tsx}"
    ],
    "exclude": [
      "**/*.test.{js,ts,tsx}",
      "**/*.test/*"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "workspaces": [
    "benchmark",
    "packages/*",
    "docs",
    "docs/packages/*",
    "test"
  ]
