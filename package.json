{
  "name": "material-ui-build",
  "private": true,
  "author": "Call-em-all Engineering Team",
  "version": "0.16.0-alpha",
  "description": "Material Design UI components built with React",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/callemall/material-ui.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/callemall/material-ui/issues"
  },
  "homepage": "http://material-ui.com/",
  "scripts": {
    "build": "npm run build:babel && npm run build:copy-files",
    "build:icon-index": "babel-node ./scripts/icon-index-generator.js",
    "build:babel": "babel ./src --ignore *.spec.js --out-dir ./build",
    "build:copy-files": "babel-node ./scripts/copy-files.js",
    "build:docs": "babel-node ./scripts/build-api-docs.js",
    "clean": "npm run clean:build",
    "clean:docs": "rimraf docs/api/*",
    "clean:build": "rimraf build",
    "docs:start": "(cd docs/site; npm start)",
    "lint": "eslint src docs/site/src test/integration && echo \"eslint: no lint errors\"",
    "lint:find-rules": "eslint-find-rules -u .eslintrc.js",
    "prebuild": "npm run clean:build",
    "test": "node test mocha",
    "test:watch": "node test mocha -w",
    "test:coverage": "istanbul cover test/index.js -- mocha",
    "test:karma": "node test karma",
    "test:e2e": "node test e2e -e chrome_51",
    "test:e2e:local": "node test e2e -l -e chrome",
    "test:e2e:all": "node test e2e -e chrome_51,safari_9,firefox_46,ie_edge,ie_10",
    "test:regressions": "node test regressions -l",
    "flow": "flow"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-tap-event-plugin": "^1.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "dom-helpers": "^2.4.0",
    "keycode": "^2.1.1",
    "lodash": "^4.13.1",
    "object-assign": "^4.1.0",
    "react-addons-create-fragment": "^15.2.0",
    "react-addons-transition-group": "^15.2.0",
    "react-event-listener": "^0.2.1",
    "recompose": "^0.20.2",
    "stylishly": "^0.8.0",
    "stylishly-at-rules": "^0.8.0",
    "stylishly-nested": "^0.8.1",
    "stylishly-pseudo-classes": "^0.8.0",
    "stylishly-units": "^0.8.0",
    "stylishly-vendor-prefixer": "^0.8.1",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@nmarks/react-docgen": "^2.9.1",
    "app-module-path": "^1.0.6",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-remove-properties": "^0.2.3",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.6",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.6.1",
    "blink-diff": "^1.0.13",
    "chai": "^3.5.0",
    "commander": "^2.9.0",
    "coveralls": "^2.11.9",
    "cross-env": "^2.0.0",
    "doctrine": "^1.2.2",
    "enzyme": "^2.2.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-find-rules": "^1.11.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flowtype": "^2.3.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-material-ui": "./packages/eslint-plugin-material-ui",
    "eslint-plugin-mocha": "^4.1.0",
    "eslint-plugin-react": "^6.0.0",
    "eventsource-polyfill": "^0.9.6",
    "flow-bin": "^0.30.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.0.3",
    "http-server": "^0.9.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^3.0.1",
    "ngrok": "^2.2.2",
    "nightwatch": "^0.9.5",
    "nodemon": "^1.9.1",
    "phantomjs-prebuilt": "^2.1.7",
    "png-crop": "0.0.1",
    "random-words": "0.0.1",
    "raw-loader": "^0.5.1",
    "react": "^15.2.0",
    "react-a11y": "^0.3.3",
    "react-addons-perf": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-tap-event-plugin": "^1.0.0",
    "recast": "^0.11.8",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "webpack": "^1.13.1"
  }
}
