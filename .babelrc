{
  "presets": ["es2015", "stage-1", "react"],
  "plugins": [
    "./scripts/babel-transform-object-assign-require",
    "transform-dev-warning"
  ],
  "env": {
    "test": {
      "sourceMaps": "both"
    },
    "docs-production": {
      "plugins": [
        ["./scripts/babel-transform-object-assign-require""],
        "transform-react-remove-prop-types",
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    }
  }
}
